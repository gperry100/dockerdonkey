http://www.mulesoft.org/schema/mule/quartz http://www.mulesoft.org/schema/mule/quartz/current/mule-quartz.xsd">
    
    <jms:activemq-connector name="Active_MQ" username="admin" password="admin" brokerURL="tcp://ostml0158211:61616" disableTemporaryReplyToDestinations="true" doc:name="Active MQ" maxRedelivery="-1" numberOfConsumers="1" validateConnections="true">
    	<service-overrides sessionHandler="org.mule.session.NullSessionHandler"/>
    </jms:activemq-connector>
	<flow name="dockerdonkeyFlow1" doc:name="dockerdonkeyFlow1">
        <jms:inbound-endpoint doc:name="in" connector-ref="Active_MQ" queue="in">
            <jms:transaction action="ALWAYS_BEGIN"/>
        </jms:inbound-endpoint>
        <scripting:component doc:name="Groovy">
            <scripting:script engine="Groovy"><![CDATA[sleep 100]]></scripting:script>
        </scripting:component>
        <logger level="INFO" doc:name="Logger"/>
        <jms:outbound-endpoint queue="out" connector-ref="Active_MQ" doc:name="out"/>
    </flow>
